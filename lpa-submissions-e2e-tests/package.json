{
  "name": "lpa-submissions-e2e-tests",
  "version": "0.0.0",
  "description": "LPA Commission E2E tests",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run -e TAGS='not @wip'",
    "test:e2e": "npx cypress run -e TAGS='not @wip'",
    "test:e2e:postprocess": "node ./reporter.js",
    "test:demo": "npx cypress run --headed -b chrome --config baseUrl='https://tim:password@appeals-dev.planninginspectorate.gov.uk' --spec 'cypress/integration/appeal-submission-access-to-appeal-site.feature' --env demoDelay=1000",
    "test:smoke": "npx cypress run -e TAGS='not @wip and @smoketest'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "axe-core": "^4.1.1",
    "cypress": "^6.3.0",
    "cypress-axe": "^0.12.0",
    "cypress-cucumber-preprocessor": "^4.0.0"
  },
  "dependencies": {
    "cypress-ntlm-auth": "^3.1.5",
    "cypress-file-upload": "5.0.0",
    "datatables.net": "1.10.23",
    "multiple-cucumber-html-reporter": "1.18.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "./cypress/cucumber-json"
    }
  }
}
