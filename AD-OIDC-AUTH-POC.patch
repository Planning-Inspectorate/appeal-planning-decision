+          label: {
+            text: "Enter your email address",
+            classes: "govuk-label--l",
+            isPageHeading: true
+          },
+          id: "email",
+          name: "email",
+          spellcheck: false,
+          value: '',
+          errorMessage: errors['email'] and {
+            text: errors['email'].msg
+          }
+
+        }) }}
+
+        <input type="hidden" value="/logged-in" name="destinationUrl" />
+
+        {{ govukButton({
+          text: "Continue",
+          type: "submit",
+          attributes: { "data-cy":"button-continue"}
+
+        }) }}
+
+      </form>
+    </div>
+
+  </div>
+{% endblock %}
