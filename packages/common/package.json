{
  "name": "@pins/common",
  "version": "0.0.0",
  "description": "Common modules to use between services",
  "private": true,
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": ">=6.0.0 <7.0.0"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js --watch",
    "format": "prettier --write './**/*.js'",
    "lint": "eslint ./",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watchAll --runInBand --coverage",
    "documentation": "jest --coverage --reporters jest-html-reporters --coverageReporters html"
  },
  "author": "",
  "dependencies": {
    "@godaddy/terminus": "^4.7.1",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "govuk-frontend": "^3.12.0",
    "notifications-node-client": "^5.1.0",
    "nunjucks": "^3.2.3",
    "nunjucks-date-filter": "^0.1.1",
    "pino": "^6.11.3",
    "prom-client": "^13.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/user-event": "^12.8.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.2",
    "prettier": "^2.1.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  }
}
