{
	"name": "appeals-service-api",
	"version": "0.0.0",
	"private": true,
	"engines": {
		"node": ">=18.0.0 <19.0.0",
		"npm": ">=9.0.0 <10.0.0"
	},
	"main": "src/main.js",
	"prisma": {
		"schema": "src/db/schema.prisma",
		"seed": "node src/db/seed/seed-dev.js"
	},
	"scripts": {
		"db:migrate:dev": "npx prisma migrate dev",
		"db:migrate:prod": "npx prisma migrate deploy",
		"db:generate": "npx prisma generate",
		"db:seed": "npx prisma db seed",
		"db:seed:prod": "npm run db:generate && node src/db/seed/seed-prod.js",
		"gen-api-types": "node src/spec/gen-api-types.js",
		"lint": "eslint ./",
		"lint:fix": "eslint ./ --fix",
		"start": "node ./",
		"start:dev": "npm run db:generate && nodemon -e .js,.json,.yml,.yaml ./ | pino-pretty -tlc",
		"start:dev:debug": "npm run db:generate && nodemon --inspect=0.0.0.0 ./",
		"test": "jest",
		"test:cov": "jest --coverage --verbose",
		"test:watch": "jest --watchAll --runInBand",
		"integration-test": "npm run db:generate && cross-env NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.developer-test.config.js"
	},
	"author": "Foundry4 <www.foundry4.com>",
	"dependencies": {
		"@azure/app-configuration": "^1.3.1",
		"@azure/identity": "3.0.1",
		"@azure/service-bus": "^7.7.3",
		"@babel/core": "^7.20.12",
		"@cloudamqp/amqp-client": "^2.1.0",
		"@pins/business-rules": "file:../business-rules",
		"@pins/common": "file:../common",
		"@prisma/client": "^5.6.0",
		"amqp-1.0-client": "^0.2.0",
		"applicationinsights": "^2.9.0",
		"axios": "^1.6.5",
		"body-parser": "^1.19.1",
		"body-parser-csv": "^1.1.0",
		"compression": "^1.7.4",
		"cross-env": "^7.0.3",
		"csv-parse": "^5.3.0",
		"date-fns": "^2.28.0",
		"express": "^4.17.2",
		"express-async-errors": "^3.1.1",
		"express-openapi-validator": "^5.1.2",
		"express-pino-logger": "^5.0.0",
		"got": "^12.5.3",
		"joi": "^17.10.2",
		"js-yaml": "^3.14.0",
		"jsonpath": "^1.1.1",
		"ky": "^0.32.2",
		"ky-universal": "^0.11.0",
		"lodash": "^4.17.21",
		"mongodb": "^3.7.3",
		"multer": "^1.4.5-lts.1",
		"neat-csv": "^5.2.0",
		"node-cron": "^3.0.2",
		"pino": "^6.13.4",
		"pino-pretty": "^4.8.0",
		"prettier": "^2.8.8",
		"rhea": "^1.0.24",
		"secure-pin": "^1.0.14",
		"supertest": "^6.2.2",
		"swagger-jsdoc": "^6.2.8",
		"swagger-typescript-api": "^13.0.3",
		"swagger-ui-express": "^4.3.0",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@shelf/jest-mongodb": "^1.3.4",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-jest": "^24.4.0",
		"i": "^0.3.7",
		"jest": "^29.4.0",
		"jest-junit": "^13.2.0",
		"jest-when": "^3.5.0",
		"mockserver-client": "^5.14.0",
		"mockserver-node": "^5.14.0",
		"nodemon": "^2.0.15",
		"path": "^0.12.7",
		"prisma": "^5.6.0",
		"rhea-promise": "^2.1.0",
		"testcontainers": "^9.8.0"
	},
	"imports": {
		"#db-client": "./src/db/db-client.js",
		"#errors/*": "./src/errors/*.js",
		"#lib/*": "./src/lib/*.js",
		"#repositories/*": "./src/repositories/*.js",
		"#middleware/*": "./src/middleware/*.js"
	}
}
