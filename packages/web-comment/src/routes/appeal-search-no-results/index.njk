{% extends "../../index.njk" %}
{% set title = ["Appeal ", appealReference] | join %}
{% from "govuk/components/input/macro.njk" import govukInput %}
{% from "govuk/components/button/macro.njk" import govukButton %}
{% block content %}
    <form method="post">
        <div class="govuk-form-group">
            <label class="govuk-label govuk-label--l" for="appeal-reference">Appeal {{ appealReference }}</label>
            <p class="govuk-body">We could not find an appeal for this reference number.</p>
            <p class="govuk-body">
                <a href="enter-appeal-reference" class="govuk-link">Find a planning appeal</a>
            </p>
        </div>
    </form>
{% endblock content %}
