{
  "name": "openfaas-amqp1.0-connector",
  "version": "1.1.1",
  "description": "An OpenFaaS connector for working with AMQP 1.0",
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": ">=6.0.0 <7.0.0"
  },
  "main": "./dist/app",
  "scripts": {
    "prebuild": "rm -Rf ./dist",
    "build": "tsc",
    "commit": "cz",
    "commitlint": "commitlint --to HEAD",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "semantic-release": "semantic-release",
    "start": "node ./",
    "start:dev": "nodemon -e .ts,.js,.json -x ts-node ./src/app | pino-pretty -tlc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "express": "^4.17.1",
    "pino": "^6.8.0",
    "pino-debug": "^1.3.0",
    "rhea": "^1.0.24"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.5",
    "@semantic-release/npm": "^7.0.8",
    "@types/express": "^4.17.9",
    "@types/pino": "^6.3.4",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "commitizen": "^4.2.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "semantic-release-monorepo": "^7.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "openfaas",
    "amqp"
  ],
  "author": "Simon Emms <simon@simonemms.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/MrSimonEmms/openfaas-amqp1.0-connector/issues"
  },
  "homepage": "https://gitlab.com/MrSimonEmms/openfaas-amqp1.0-connector#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
