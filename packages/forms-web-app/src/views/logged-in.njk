{% from "govuk/components/error-summary/macro.njk" import govukErrorSummary %}

{% set title="Check your inbox - Appeal a householder planning decision - GOV.UK" %}

{% block pageTitle %}
  {% if errors %}
    {% set title="Error: " + title %}
  {% endif %}
  {{ title }}
{% endblock %}

{% block content %}


  <div class="govuk-grid-row">

    <div class="govuk-grid-column-two-thirds">
      <h3>Logged in as {{userName}}</h3>
      <p> You have logged in without any destination given ...... </p>
      <p>We could show you allsorts here ... like all appeals associated with your email address or other things</p>
    </div>

  </div>
{% endblock %}
