{% extends layoutTemplate %}
{% from "govuk/components/summary-list/macro.njk" import govukSummaryList %}
{% from "govuk/components/tag/macro.njk" import govukTag %}
{% from "govuk/components/button/macro.njk" import govukButton %}



{% block pageTitle %}
  Appeal Submitted - Appeal a planning decision - GOV.UK
{% endblock %}

{% block content %}

  <style {% if cspNonce %}nonce={{ cspNonce }}{% endif %}>{{ css | safe }}</style>
  <style {% if cspNonce %}nonce={{ cspNonce }}{% endif %}>.govuk-template { background-color: #FFFFFF; }</style>

	<div class="govuk-grid-row">
    <div class="govuk-grid-column-full">
        <h1 class="govuk-heading-l">
            Appeal number TO BE ADDED
				</h1>
				<p class="govuk-body">Submitted on {{submissionDate}}.</p>

      {% for section in summaryListData.sections %}
            <h2 class="govuk-heading-m">
              {{section.heading}}
            </h2>

        {% if section.list.rows.length %}
          {{ govukSummaryList(section.list) }}
        {% endif %}
      {% endfor %}

    </div>
  </div>

{% endblock %}
