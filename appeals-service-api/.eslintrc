{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["jest", "prettier"],
  "root": true,
  "ignorePatterns": ["node_modules/**","src/public/javascript/**"],
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  }
}








